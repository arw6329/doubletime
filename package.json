{
    "name": "doubletime",
    "version": "1.2.1",
    "description": "Validate object schemas with compile-time and runtime type safety",
    "main": "dist/index-cjs.js",
    "module": "dist/index.js",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index-cjs.js"
    },
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "rollup -c",
        "test": "jest --maxWorkers=2",
        "test-main": "jest ./src/entrypoint.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arw6329/doubletime.git"
    },
    "keywords": [
        "schemas",
        "types",
        "type validation",
        "schema validation",
        "object validation",
        "type safety",
        "runtime types",
        "object schemas",
        "object type inference"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arw6329/doubletime/issues"
    },
    "homepage": "https://github.com/arw6329/doubletime#readme",
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "jest": "^29.7.0",
        "rollup": "^4.34.9",
        "rollup-plugin-dts": "^6.1.1",
        "ts-jest": "^29.2.6",
        "tslib": "^2.8.1",
        "typescript": "^5.8.2"
    }
